{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@secrets": ["./secrets.ts"],
    },
    "moduleResolution": "node",
    "outDir": "./dist",
    "target": "es5",
    "module": "es6",
    "jsx": "react",
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true
  },
  "exclude": ["node_modules", "dist"]
